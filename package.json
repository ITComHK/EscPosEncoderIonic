{
  "name": "esc-pos-encoder-ionic",
  "version": "1.1.1",
  "description": "Create a set of commands that can be send to any receipt printer that supports ESC/POS. Support Ionic (Typescript)",
  "main": "src/esc-pos-encoder-ionic.js",
  "scripts": {
    "bundle": "browserify src/esc-pos-encoder-ionic.js --standalone EscPosEncoderIonic -o dist/esc-pos-encoder-ionic.js",
    "uglify": "uglifyjs dist/esc-pos-encoder-ionic.js -cm --overwrite -o dist/esc-pos-encoder-ionic.js",
    "build": "npm run bundle && npm run uglify",
    "lint": "eslint --fix src/esc-pos-encoder-ionic.js",
    "mocha": "mocha test/",
    "test": "npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITComHK/EscPosEncoderIonic.git"
  },
  "keywords": [
    "pos",
    "escpos",
    "receipt",
    "printer",
    "ionic",
    "ionic4",
    "ionic5"
  ],
  "author": "Charles JokGa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ITComHK/EscPosEncoderIonic/issues"
  },
  "homepage": "https://github.com/ITComHK/EscPosEncoderIonic#readme",
  "devDependencies": {
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^4.1.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "buffer-to-uint8array": "^1.1.0",
    "canvas": "^2.6.1",
    "canvas-dither": "^1.0.0",
    "canvas-flatten": "^1.0.0",
    "iconv-lite": "^0.4.19",
    "linewrap": "^0.2.1"
  }
}
